<!DOCTYPE html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Korex Hospital</title>
    <!-- base:css -->
    <link rel="stylesheet" href="../../font/typicons.css">
    <link rel="stylesheet" href="../../css/vendor.bundle.base.css">
    <link rel="stylesheet" href="../../vertical-layout-light/style.css">
    <!-- endinject -->
    <link rel="icon" href="../../img/logos.jpg">
  </head>
  <body>
    <div class="container-scroller">
      <!-- partial:partials/_navbar.html -->
      <nav class="navbar col-lg-12 col-12 p-0 fixed-top d-flex flex-row">

        <div class="text-center navbar-brand-wrapper d-flex align-items-center justify-content-center">
          <a class="navbar-brand brand-logo" href="/backend/dashboard"><img src="../../img/logos.jpg" alt="logo"/></a>
          <a class="navbar-brand brand-logo-mini" href="/backend/dashboard"><img src="../../img/logos.jpg" alt="logo"/></a>
          <button class="navbar-toggler navbar-toggler align-self-center d-none d-lg-flex" type="button" data-toggle="minimize">
            <span class="typcn typcn-th-menu"></span>
          </button>
        </div>

        <%- include ('../partials/adminNav'); -%>
      </nav>
      <!-- partial -->
      <div class="container-fluid page-body-wrapper">
        <!-- partial:partials/_settings-panel.html -->
        <div class="theme-setting-wrapper">
          <div id="settings-trigger"><i class="typcn typcn-cog-outline"></i></div>
          <div id="theme-settings" class="settings-panel">
            <i class="settings-close typcn typcn-delete-outline"></i>
            <p class="settings-heading">SIDEBAR SKINS</p>
            <div class="sidebar-bg-options" id="sidebar-light-theme">
              <div class="img-ss rounded-circle bg-light border mr-3"></div>
              Light
            </div>
            <div class="sidebar-bg-options selected" id="sidebar-dark-theme">
              <div class="img-ss rounded-circle bg-dark border mr-3"></div>
              Dark
            </div>
            <p class="settings-heading mt-2">HEADER SKINS</p>
            <div class="color-tiles mx-0 px-4">
              <div class="tiles success"></div>
              <div class="tiles warning"></div>
              <div class="tiles danger"></div>
              <div class="tiles primary"></div>
              <div class="tiles info"></div>
              <div class="tiles dark"></div>
              <div class="tiles default border"></div>
            </div>
          </div>
        </div>
        <!-- partial -->
        <!-- partial:partials/_sidebar.html -->
        <%- include('../partials/adminSidebar'); -%>

        <!-- partial -->
        <div class="main-panel">
          <div class="content-wrapper">

          
            <div class="col-lg-12 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title">Property in <%=proInfo.address%> <%=proInfo.city%> <%=proInfo.state%> Profile</h4>

                        <ul class="v">
                            <li><strong>Property Image</strong></li>
                            <p>
                              <!-- Render the table here showing images in array -->
                              <% if (proInfo.images && proInfo.images.length > 0) { %>
                                <% proInfo.images.forEach(image => { %>
                                    <img class="propertimg" src="data:<%= image.contentType %>;base64,<%= image.data.toString('base64') %>">
                                <% }); %>
                            <% } %>
                          </p>
                            <li><strong>Agent In charge</strong></li>
                            <p>
                                <%=proInfo.contactPerson%>
                            </p>
                            <li><strong>Agent Number</strong></li>
                            <p>
                                <%=proInfo.agentNumber%>
                            </p>
                            <li><strong>Property Type</strong></li>
                            <p>
                                <%=proInfo.propertyType%>
                            </p>
                            <li><strong>Property Amount</strong></li>
                            <p>
                                <%=proInfo.amount%>
                            </p>
                            <li><strong>Property Condition</strong></li>
                            <p>
                                <%=proInfo.houseCondition%>
                            </p>
                            <li><strong>Property Status</strong></li>
                            <p>
                                <%=proInfo.propertyStatus%>
                            </p>
                            <li><strong>Property Heading</strong></li>
                            <p>
                                <%=proInfo.heading %>
                            </p>
                            <li><strong>Property Size in feet</strong></li>
                            <p>
                                <%=proInfo.sizeinFt%>
                            </p>
                            <li><strong>Bedrooms</strong></li>
                            <p>
                                <%=proInfo.bedrooms %>
                            </p>
                            <li><strong>Bathrooms</strong></li>
                            <p>
                                <%=proInfo.bathrooms %>
                            </p>
                            <li><strong>Garage</strong></li>
                            <p>
                                <%=proInfo.garages %>
                            </p>
                            <li><strong>Swimming Pool</strong></li>
                            <p>
                                <%=proInfo.pool %>
                            </p>
                            <li><strong>Built Year</strong></li>
                            <p>
                                <%=proInfo.yearBuilt %>
                            </p>
                            <li><strong>Available From</strong></li>
                            <p>
                                <%=proInfo.availableFrom %>
                            </p>
                            <li><strong>Description</strong></li>
                            <p>
                                <%=proInfo.description %>
                            </p>
                            
                        </ul>
                      
                       
                    </div>
                </div>
        
            </div>
   
            <%- include (`../partials/adminFooter`); -%>

        </div>

      </div>

    </div>




  </body>
</html>